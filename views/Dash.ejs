            <script>
              const resultArray = [0, 0, 0, 0, 0, 0];

              values.forEach(value => {
                if (value >= 95 && value <= 1000) {
                  resultArray[0]++;
                } else if (value >= 90 && value < 95) {
                  resultArray[1]++;
                } else if (value >= 85 && value < 90) {
                  resultArray[2]++;
                } else if (value >= 80 && value < 85) {
                  resultArray[3]++;
                } else if (value >= 75 && value < 80) {
                  resultArray[4]++;
                } else {
                  resultArray[5]++;
                }
              });

              console.log(resultArray);
